var speed = 10.50;
var age = 10;
var firstName = "Armando";


console.log(speed > 11 && age < 11);

console.log(speed >9 || age < 12);
console.log(speed == age);
console.log(speed === age);
console.log(speed !== age);
console.log(!(age > 18));

// int, float, string , boolean , undefined, null 
// ==, ===
// >, <, >=, <=

//Enteros
console.log(10 > 4);
console.log(3 < 3);
console.log (5 >= 5);
console.log (4 <= 2);

console.log(6 == 7);
console.log(9 === 9);







var numberOne = 10;
var numberTwo = 25.60;


console.log(numberOne + numberTwo);
console.log(numberOne - numberTwo);
console.log(numberOne * numberTwo);
console.log(numberOne / numberTwo);
console.log(numberOne % numberTwo);

